
Array.prototype.myFilter = function (callback) {
  // выходной массив. Его функция вернет по завершении
  var result = []
  // вызываем функцию колбэк с элементом массива в качестве аргумента
  for (var i = 0; i < this.length; i++) {
    // если результат колбэка вернул true, то добавляем элемент исходного массива в выходной массив.
    if (callback(this[i]) === true) {
      result.push(this[i])
    }
  }
  return result;
};

var arr = [-1,2,-3,4,5,-6]; // этот массив будет this'ом для метода myFilter
// функция для фильтрации
function findPositive(item) {
  if (item > 0) {
    return true;
  }
  return false;
}

alert(arr.myFilter(findPositive));